	pushMerge = group user/Marcin Czech (maczech)
[access "refs/tags/*"]
	push = +force group user/Marcin Czech (maczech)
	createSignedTag = group user/Marcin Czech (maczech)
	createTag = group user/Marcin Czech (maczech)
