[project]
	description = Patched, refactored version of code.google.com/hadoop-gpl-compression for hadoop 0.20
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Marcin Czech (maczech)
[access "refs/for/refs/*"]
	read = group user/Marcin Czech (maczech)
